{
  "name": "server-app",
  "version": "0.0.0",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist",
    "start:dev": "nodemon --config nodemon.json",
    "client": "cd front-application && npm start",
    "python-server": "cd python_server && python server.py",
    "start:all": "concurrently --kill-others-on-fail \"npm run python-server\" nodemon --config nodemon.json \"npm run client\"",
    "start:partial": "concurrently --kill-others-on-fail nodemon --config nodemon.json \"npm run client\"",
    "test": "nodemon --config nodemon.test.json"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/react-router-dom": "^5.1.3",
    "command-line-args": "^5.1.1",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "excel4node": "^1.7.2",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "plotly.js-gl3d-dist": "^1.52.2",
    "react-context-hook": "^1.2.0",
    "react-hooks-global-state": "^1.0.0-alpha.2",
    "react-router-dom": "^5.1.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/find": "^0.2.1",
    "@types/jasmine": "^3.5.3",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.0",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jasmine": "^3.5.0",
    "jsonfile": "^5.0.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
