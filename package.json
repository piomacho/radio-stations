{
  "name": "server-app",
  "version": "0.0.0",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist",
    "start:dev": "nodemon --config nodemon.json",
    "client": "cd front-application && npm start",
    "python-server": "cd python_server && python server.py",
    "start:all": "concurrently --kill-others-on-fail \"npm run python-server\" nodemon --config nodemon.json \"npm run client\"",
    "start:partial": "concurrently --kill-others-on-fail \"kill-port --port 5000\" nodemon --config nodemon.json \"npm run client\"",
    "test": "nodemon --config nodemon.test.json"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.6.0",
    "@types/cors": "^2.8.6",
    "@types/geopoint": "^1.0.1",
    "@types/google-map-react": "^2.1.0",
    "@types/leaflet": "^1.5.21",
    "@types/react-router-dom": "^5.1.3",
    "async": "^3.2.0",
    "axios": "^0.21.0",
    "axios-retry": "^3.1.9",
    "big-json": "^3.1.0",
    "bmp-js": "^0.1.0",
    "body-parser": "^1.19.0",
    "browserify-fs": "^1.0.0",
    "child-process": "^1.0.2",
    "command-line-args": "^5.1.1",
    "commander": "^6.2.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "deep-sort-object": "^1.0.2",
    "dotenv": "^8.2.0",
    "excel4node": "^1.7.2",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "glob": "^7.1.6",
    "google-map-react": "^2.1.9",
    "google-maps": "^4.3.3",
    "http-status-codes": "^1.4.0",
    "jimp": "^0.16.1",
    "kill-port": "^1.6.1",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "object-sizeof": "^1.6.1",
    "oboe": "^2.1.5",
    "p-limit": "^3.1.0",
