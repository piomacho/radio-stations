    "react-leaflet": "^3.0.5",
    "react-router-dom": "^5.1.2",
    "read-excel-file": "^4.0.8",
    "request-stats": "^3.0.0",
    "rimraf": "^3.0.2",
    "socket.io": "^3.0.3",
    "sort-keys": "^4.1.0",
    "sort-object-keys": "^1.1.3",
    "stream-json": "^1.7.1",
    "winston": "^3.2.1",
    "xlsx": "^0.16.8",
    "xml-js": "^1.6.11",
    "xmlbuilder": "^15.1.1"
